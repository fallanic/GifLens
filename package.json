{
	"name": "giflens",
	"displayName": "Giflens",
	"description": "Like Gitlens, but with GIFs",
	"version": "2.0.0",
	"publisher": "giflens",
	"keywords": [
		"gif",
		"vscode",
		"mbappe",
		"game of thrones",
		"lol",
		"IDE",
		"mdr",
		"code",
		"giphy"
	],
	"contributors": [
		"Fabien Allanic",
		"Maxime Burlat"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/giflens/giflens/issues"
	},
	"homepage": "https://giflens.org",
	"icon": "docs/assets/giflens-icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/giflens/GifLens.git"
	},
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "giflens.search",
				"title": "Giflens Search",
				"icon": {
					"dark": "docs/assets/icons/dark-search.svg",
					"light": "docs/assets/icons/light-search.svg"
				}
			},
			{
				"command": "giflens.addGif",
				"title": "Insert Gif",
				"icon": {
					"dark": "docs/assets/icons/dark-plus.svg",
					"light": "docs/assets/icons/light-plus.svg"
				}
			},
			{
				"command": "giflens.resetHistory",
				"title": "Reset Giflens History",
				"icon": {
					"dark": "docs/assets/icons/dark-trash-alt.svg",
					"light": "docs/assets/icons/light-trash-alt.svg"
				}
			},
			{
				"command": "giflens.deleteGif",
				"title": "Remove from History",
				"icon": {
					"dark": "docs/assets/icons/dark-trash-alt.svg",
					"light": "docs/assets/icons/light-trash-alt.svg"
				}
			},
			{
				"command": "giflens.addToFavorites",
				"title": "Add to Favorites",
				"icon": {
					"dark": "docs/assets/icons/dark-star.svg",
					"light": "docs/assets/icons/light-star.svg"
				}
			},
			{
				"command": "giflens.removeFromFavorites",
				"title": "Remove from Favorites",
				"icon": {
					"dark": "docs/assets/icons/dark-trash-alt.svg",
					"light": "docs/assets/icons/light-trash-alt.svg"
				}
			},
			{
				"command": "giflens.viewGif",
				"title": "View Gif"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "giflens.search",
					"when": "view == history",
					"group": "navigation"
				},
				{
					"command": "giflens.resetHistory",
					"when": "view == history",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "giflens.addGif",
					"when": "view == history && viewItem == historyItem",
					"group": "inline"
				},
				{
					"command": "giflens.addGif",
					"when": "view == favorites && viewItem == favoritesItem",
					"group": "inline"
				},
				{
					"command": "giflens.deleteGif",
					"when": "view == history && viewItem == historyItem",
					"group": "inline"
				},
				{
					"command": "giflens.addToFavorites",
					"when": "view == history && viewItem == historyItem",
					"group": "inline"
				},
				{
					"command": "giflens.removeFromFavorites",
					"when": "view == favorites && viewItem == favoritesItem",
					"group": "inline"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "giflens",
					"title": "Giflens",
					"icon": "docs/assets/icons/activity-bar.svg"
				}
			]
		},
		"views": {
			"giflens": [
				{
					"id": "history",
					"name": "Search History"
				},
				{
					"id": "favorites",
					"name": "Favorites"
				}
			]
		},
		"grammars": [
			{
				"path": "./syntaxes/injection.json",
				"scopeName": "giflens",
				"injectTo": [
					"source.bat",
					"source.clojure",
					"source.coffee",
					"source.c",
					"source.cpp",
					"source.cs",
					"source.css",
					"source.dockerfile",
					"source.fsharp",
					"source.go",
					"source.groovy",
					"text.html",
					"source.java",
					"source.js",
					"source.js.jsx",
					"source.less",
					"source.lua",
					"source.makefile",
					"source.objc",
					"source.objcpp",
					"source.perl",
					"source.php",
					"text.html.php",
					"source.powershell",
					"source.python",
					"source.r",
					"source.ruby",
					"source.rust",
					"source.sass",
					"source.css.scss",
					"source.sql",
					"source.swift",
					"source.ts",
					"source.tsx",
					"source.asp.vb.net",
					"source.yaml"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts, js,json,css,md}": [
			"prettier --write",
			"git add"
		]
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"husky": "^1.3.1",
		"lint-staged": "^13.1.0",
		"prettier": "1.16.4",
		"tslint": "^5.8.0",
		"typescript": "^3.1.4",
		"vscode": "^1.1.33"
	},
	"dependencies": {
		"axios": ">=0.19.0"
	}
}
